msgctxt "STRINGS.MISSIONS.JAILBREAK.GUARD"
msgid "GUARD"
msgstr "守卫"

#: STRINGS.MISSIONS.JAILBREAK.GUARDTURN_1
msgctxt "STRINGS.MISSIONS.JAILBREAK.GUARDTURN_1"
msgid "Another slow day..."
msgstr "另一个漫长的一天……"

#: STRINGS.MISSIONS.JAILBREAK.GUARDTURN_2
msgctxt "STRINGS.MISSIONS.JAILBREAK.GUARDTURN_2"
msgid "A few more hours and the interrogator should be here."
msgstr "再过几个小时审讯官就要到这里了。"

#: STRINGS.MISSIONS.JAILBREAK.GUARDTURN_3
msgctxt "STRINGS.MISSIONS.JAILBREAK.GUARDTURN_3"
msgid "..."
msgstr "……"

#: STRINGS.MISSIONS.JAILBREAK.INITIATING
msgctxt "STRINGS.MISSIONS.JAILBREAK.INITIATING"
